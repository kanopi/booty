{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}
{% if items.next %}
  <ul{{ attributes }}>
    <li class="pager__item">
      <a href="{{ items.next.href }}" title="{{ 'Load more'|t }}" rel="next"><span class="visually-hidden">{{ options.button_text }}</span></a>
    </li>
  </ul>
{% endif %}
